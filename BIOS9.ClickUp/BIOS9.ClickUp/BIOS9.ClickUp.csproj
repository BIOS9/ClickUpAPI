<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Authors>BIOS9</Authors>
        <PackageProjectUrl></PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/BIOS9/ClickUpAPI/blob/main/LICENSE.md</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/BIOS9/ClickUpAPI</RepositoryUrl>
        <Description>An unofficial .NET wrapper for the ClickUp V2 API. </Description>
        <RepositoryType>git</RepositoryType>
        <PackageIcon>icon.png</PackageIcon>
        <PackageTags>clickup api</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
      <PackageReference Include="RestSharp" Version="110.2.0" />
    </ItemGroup>

    <ItemGroup>
      <None Update="icon.png">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
      </None>
    </ItemGroup>

</Project>
